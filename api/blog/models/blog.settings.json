      "required": true
    },
    "createdAt": {
      "type": "time",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "header": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "author": {
      "model": "author",
      "via": "blogs"
    }
  }
}
